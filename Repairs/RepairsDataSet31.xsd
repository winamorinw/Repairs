                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--SELECT os.b_bal_cena, NumberPto, ednome, pto.name as name, quantity, mera, prace, summa, tehno, norma, --vedomostnumber, pto.inventoryNumber as inventoryNumber , NameOs, Obect, kol_1_1, sum_1_1, kol_1_2, --sum_1_2, kol_2_1, sum_2_1, kol_2_2, sum_2_2, priznak ,srok_zameni,sz.nam_zameni
--FROM dbo.ProizvodTehOtchetWithNomer(@idpto) pto
--left join SprOS os on pto.inventoryNumber=os.InventoryNumber  and  pto.Obect=os.Obekt left join
 --Srok_zameni sz on pto.srok_zameni=sz.id_zameni
SELECT os.b_bal_cena , NumberPto, ednome, pto.name as name, quantity, mera, prace, summa, tehno, norma, vedomostnumber, pto.inventoryNumber as inventoryNumber , 
NameOs, Obect, kol_1_1, sum_1_1, kol_1_2, sum_1_2, kol_2_1, sum_2_1, kol_2_2, sum_2_2, priznak ,srok_zameni,sz.nam_zameni,'1pto' as namdoc
FROM dbo.ProizvodTehOtchetWithNomer(@idpto) pto
left join SprOS os on pto.inventoryNumber=os.InventoryNumber  and  pto.Obect=os.Obekt left join Srok_zameni sz on pto.srok_zameni=sz.id_zameni
--union
--SELECT  '' as   b_bal_cena,  cast(PTO.NomPTO as int) as numberpto, VozvrDet.EdNumber as --ednome,VozvrDet.Name
--	,  VozvrDet.Quantity , '' as mera , 0 as prace , 0 as summa, 0 as tehno , 0 as norma
--	, Vedomost_Resursov.Id as VedomostNumber, Vedomost_Resursov.InventoryNamber as --InventoryNamber, 
--	--'' as NameOs
--	NameOs = 'Інв. №' + CAST(Vedomost_Resursov.InventoryNamber as nvarchar(15)) + '  '+ CAST--(Vedomost_Resursov.Obekt as nvarchar(15)) + '  ' + Vedomost_Resursov.NameOs
--	, '' as Obect
--	, null as kol_1_1,null as sum_1_1, null as kol_1_2,null as sum_1_2,null as kol_2_1,null as sum_2_1
--	, null as kol_2_2,null as sum_2_2, '' as priznak	, '' as srok_zameni	, '' as nam_zameni
--	 ,'2det' as namdoc
--FROM            PTO 
--INNER JOIN Vedomost_Resursov ON PTO.IdPTO = Vedomost_Resursov.IdPTO 
--INNER JOIN VozvrDet ON Vedomost_Resursov.RepairID = VozvrDet.RepairID
--WHERE        (PTO.IdPTO =@idpto)
union all
SELECT    '' as   b_bal_cena, cast( PTO.NomPTO as int)as numberpto , VozvrMet.EdNumber as ednome, VozvrMet.Name
	 , VozvrMet.Quantity, '' as mera, VozvrMet.Price as prace , VozvrMet.Sum as summa,
    0 as tehno , 0 as norma 
	, Vedomost_Resursov.Id as VedomostNumber, Vedomost_Resursov.InventoryNamber as InventoryNamber, 
	
	--'' as NameOs, 
	  NameOs = 'Інв. №' + CAST(Vedomost_Resursov.InventoryNamber as nvarchar(15)) + '  '+ CAST(Vedomost_Resursov.Obekt as nvarchar(15)) + '  ' + Vedomost_Resursov.NameOs,
	'' as Obect
	, null as kol_1_1, null as sum_1_1, null as kol_1_2, null as sum_1_2, null as kol_2_1, null as sum_2_1
	, null as kol_2_2, 0 as sum_2_2, null as priznak, '' as srok_zameni, '' as nam_zameni
		 ,'3met' as namdoc
FROM            PTO 
INNER JOIN Vedomost_Resursov ON PTO.IdPTO = Vedomost_Resursov.IdPTO 
INNER JOIN VozvrMet ON Vedomost_Resursov.RepairID = VozvrMet.RepairID and Vedomost_Resursov.ID  = VozvrMet.VedomostNumber
WHERE        (PTO.IdPTO = @idpto) 
order by inventoryNumber,namdoc</CommandText>
